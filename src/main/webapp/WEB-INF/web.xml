<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>SimpleServlet</servlet-name>
    <servlet-class>appo1c.SimpleServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SimpleServlet</servlet-name>
    <url-pattern>/simple</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>WelcomeServlet</servlet-name>
    <servlet-class>appo1c.WelcomeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>WelcomeServlet</servlet-name>
    <url-pattern>/welcome</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>top10</servlet-name>
    <servlet-class>app02a.Top10Servlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>top10</servlet-name>
    <url-pattern>/top10</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>preference</servlet-name>
    <servlet-class>app02d.PreferenceServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>preference</servlet-name>
    <url-pattern>/preference</url-pattern>
  </servlet-mapping>
</web-app>
